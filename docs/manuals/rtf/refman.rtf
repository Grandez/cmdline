{\cf18 void} printOption({\cf18 string} option, vector<string> values) \{\par
   {\cf18 int} num = 0;\par
   cout << {\cf22 "\\t"} << option << {\cf22 "\\t: "}; \par
   {\cf19 if} (values.size() == 1 && values[0].length() == 0) \{\par
       cout << {\cf22 "No default value\\n"};\par
       {\cf19 return};\par
   \}\par
   {\cf19 for} ({\cf17 auto} str : values) \{\par
        {\cf19 if} (num) cout << {\cf22 ", "};\par
        cout << str;\par
        num++;\par
   \}\par
   cout << endl;\par
\}\par
\par
{\cf18 void} showHelp({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAS" }{}}{\fldrslt {\cs37\ul\cf2 HelpRequested}}}
* help) \{\par
    cout << {\cf22 "A sample program about error management"}                << endl;\par
    cout << {\cf22 "Use: "} << help->name << {\cf22 " [options]"}                    << endl;\par
    cout << {\cf22 "Options:"}                                               << endl;\par
    cout << {\cf22 "\\t"} << VERBOSE   << {\cf22 ":\\t"}   << {\cf22 "Flag"}                              << endl;\par
    cout << {\cf22 "\\t"} << SUMMARY   << {\cf22 ":\\t"}   << {\cf22 "Flag"}                              << endl;\par
    cout << {\cf22 "\\t"} << FLAG      << {\cf22 ":\\t\\t"} << {\cf22 "Option boolean"}                    << endl;\par
    cout << {\cf22 "\\t"} << NUMBERS   << {\cf22 ":\\t"}   << {\cf22 "Integers"}                          << endl;\par
    cout << {\cf22 "\\t"} << {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAL" }{}}{\fldrslt {\cs37\ul\cf2 NUMBER}}}
    << {\cf22 ":\\t\\t"} << {\cf22 "Just one integer"}                  << endl;\par
    cout << {\cf22 "\\t"} << DECIMALS  << {\cf22 ":\\t"}   << {\cf22 "Decimals"}                          << endl;\par
    cout << {\cf22 "\\t"} << {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAM" }{}}{\fldrslt {\cs37\ul\cf2 DECIMAL}}}
   << {\cf22 ":\\t"}   << {\cf22 "Just one decimal"}                  << endl;\par
    cout << {\cf22 "\\t"} << {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAN" }{}}{\fldrslt {\cs37\ul\cf2 DATE}}}
      << {\cf22 ":\\t\\t"} << {\cf22 "Date"}                              << endl;\par
    cout << {\cf22 "\\t"} << {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAO" }{}}{\fldrslt {\cs37\ul\cf2 TIME}}}
      << {\cf22 ":\\t\\t"} << {\cf22 "Time"}                              << endl;\par
    cout << {\cf22 "\\t"} << {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAP" }{}}{\fldrslt {\cs37\ul\cf2 DATETIME}}}
  << {\cf22 ":\\t"}   << {\cf22 "DateTime"}                          << endl;\par
    cout << {\cf22 "\\t"} << TIMESTAMP << {\cf22 ":\\t"}   << {\cf22 "Timestamp"}                         << endl;\par
    cout << {\cf22 "\\t"} << DIRIN     << {\cf22 ":\\t\\t"} << {\cf22 "Existing directories"}              << endl;\par
    cout << {\cf22 "\\t"} << DIROUT    << {\cf22 ":\\t\\t"} << {\cf22 "A directory"}                       << endl;\par
    cout << {\cf22 "\\t"} << FILEIN    << {\cf22 ":\\t\\t"} << {\cf22 "Existing files"}                    << endl;\par
    cout << {\cf22 "\\t"} << FILEOUT   << {\cf22 ":\\t"}   << {\cf22 "A possible output file"}            << endl;\par
    cout << {\cf22 "\\t"} << CONFIG    << {\cf22 ":\\t\\t"} << {\cf22 "An existing configuration file"}    << endl;\par
\}\par
\par
{\cf18 void} showValues() \{\par
    Flags   flags   = cmdLine->getCurrentFlags();\par
    Options options = cmdLine->getCurrentOptions();\par
    cout << {\cf22 "Flags:"} << endl;\par
    {\cf19 for} ({\cf17 auto} flag : flags) \{\par
       cout << {\cf22 "\\t"} << flag.first << {\cf22 ": "} << std::boolalpha << flag.second << endl;\par
    \}\par
    cout << {\cf22 "Options:"} << endl;\par
    {\cf19 for} ({\cf17 auto} option : options) \{\par
        printOption(option.first, option.second);\par
    \}\par
    \par
\}\par
}
